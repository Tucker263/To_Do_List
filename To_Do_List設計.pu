@startuml
'Modelの設計
class Task{
    - int ordinalNumber
    - String title

    + Task(ordinalNumber:int, title: String)
}

class VueToDoList{
    - String title
    - Task[] taskList
    - int taskCreateCount= 0

    + VueToDoList()
    + addTask(title: String)
    + canAddTask(): boolean
    + deleteTask(ordinalNumber: int): void
}

@enduml